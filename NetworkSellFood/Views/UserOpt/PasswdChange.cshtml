@model NetworkSellFood.WebUser

@{
    ViewBag.Title = "密码修改";
    Layout = "~/Views/Shared/userinfo.cshtml";
}
@section headuser{
<link href="/Content/css/chgpwd.css" type="text/css" rel="stylesheet" />
<script src="/Content/scripts/chgpwd.js" type="text/javascript"></script>
}
<div class="panlarea">
<form role="form" class="form-group">
<ul>
<li><label class="labels">旧密码:</label><input type="password" name="oldpassword" id="txtoldpwd" maxlength="20" placeholder="请输入旧密码"  class="form-control" /></li>
<li><label class="labels">新密码:</label><input type="password" name="password" id="txtpwd" maxlength="20" placeholder="请输入新密码"  class="form-control" /></li>
<li><label class="labels">确认密码:</label><input type="password" name="passwordagn" id="txtpwda" maxlength="20" placeholder="请输入确认密码"  class="form-control" /></li>
<li><button id="btnsubmit" type="button" class="btn btn-primary">提交</button></li>
</ul>
</form>
</div>

